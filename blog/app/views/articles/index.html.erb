
<h1>Hello, Rails!</h1>
<%= link_to 'My Blog', controller: 'articles' %>
<h2>Lista de Textos</h2>
<%= link_to 'New Article', new_article_path %>
<table>
    <tr>
        <th>Title</th>
        <th>Text</th>
    </tr>
    
    <% @article.each do |article| %>
        <tr>
            <td><%= article.title %></td>
            <td><%= article.text %></td>
        </tr>
    <% end %>
    
</table>